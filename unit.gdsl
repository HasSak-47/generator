model ServerState {
    dname: string,
    iname: string,

    down: bool,
    available: bool,
}

model UnitState {
    dname: string,
    iname: string,

    servers: ServerState[],
}

get_unit_state(iname: string) @get "/api/v1/unit/{iname}/state" -> UnitState
get_unit_id(iname: string) @get "/api/v1/unit/{iname}/id" -> uint
